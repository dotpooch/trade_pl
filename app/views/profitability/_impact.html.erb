<table id="sortable_list" class="table table-striped table-bordered table-condensed">

  <thead>

    <tr>
      <th colspan="2">Impact</th>
    </tr>

  </thead>

  <tbody>

    <tr>
      <%= td("Average Gain : Average Loss") %>
      <%= td_deci(cycle.impact,2) %>
    </tr>

    <tr>
      <%= td("Unit Gains : Unit Losses") %>
      <%= td_deci(cycle.unit_impact,2) %>
    </tr>

    <tr>
      <%= td("% Gain : % Loss") %>
      <%= td_deci(cycle.return_impact,2) %>
    </tr>

  </tbody>

</table>
