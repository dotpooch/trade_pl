<h3>price data</h3>

<table class="table table-striped table-bordered table-condensed">
  
<thead>
  <tr>
    <%= th("Price","Date") %>
    <%= th("Price","Open") %>
    <%= th("Price","High") %>
    <%= th("Price","Low") %>
    <%= th("Price","Close") %>
    <%= th("Price","Volume") %>
    <%= th("Price","Adjusted Close") %>
  </tr>
</thead>

<tbody>

<% @prices.each do |price| %>
  <tr>
    <%= td_short_date(price.date) %>
    <%= td_money(price.open,2) %>
    <%= td_money(price.high,2) %>
    <%= td_money(price.low,2) %>
    <%= td_money(price.close,2) %>
    <% unless price.volume.nil? %>
      <td>-</td>
    <% else %>
      <%= td_big_int(price.volume) %>
    <% end %>
    <%= td_money(price.adj_close,2) %>
  </tr>
<% end %>

</tbody>
</table>


