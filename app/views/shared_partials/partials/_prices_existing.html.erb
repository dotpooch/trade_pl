<h3>price data</h3>
  
<table class="table table-striped table-bordered table-condensed">
  
<thead>
  <tr>
    <th class="list_item_price_date">date</th>
    <th class="list_item_price_open">open</th>
    <th class="list_item_price_high">high</th>
    <th class="list_item_price_low">low</th>
    <th class="list_item_price_close">close</th>
    <th class="list_item_price_volume">volume</th>
    <th class="list_item_price_adjusted_close">adj close</th>
  </tr>
</thead>

<tbody>

<% @security.recent_prices_desc.each do |price| %>
  <tr>
    <td class="list_item_price_date"><%= price.date %></td>
    <%= td_money(price.open) %>
    <%= td_money(price.high) %>
    <%= td_money(price.low) %>
    <%= td_money(price.close) %>
    <%= td_big_int(price.volume) %>
    <%= td_money(price.adjusted_close) %>
  </tr>
<% end %>

</tbody>
</table>


