<h3>order transactions</h3>
  
<table class="table table-striped table-bordered table-condensed">
  
<thead>
  <tr>
    <th class="list_item_transaction_type">type</th>
    <th class="list_item_transaction_date">date</th>
    <th class="list_item_transaction_symbol">symbol</th>
    <th class="list_item_transaction_name">name</th>
    <th class="list_item_transaction_quantity">quantity</th>
    <th class="list_item_transaction_trade_stub">trade</th>
    <th class="list_item_transaction_price">price</th>
    <th class="list_item_transaction_gross_principal">gross principal</th>
    <th class="list_item_transaction_commission">commission</th>
    <th class="list_item_transaction_sec_fee">SEC fee</th>
    <th class="list_item_transaction_total_fees">total fees</th>
    <th class="list_item_transaction_net_principal">net principal</th>
    <th class="list_item_transaction_jottings">jottings</th>
  </tr>
</thead>

<tbody>

<% @transactions.each do |transaction| %>
  <tr>
    <td class="list_item_transaction_type"><%= transaction.fill_type %></td>
    <%= td_long_date(transaction.date) %>
    <td><%= transaction.symbol %></td>
    <td><%= link_to transaction.stub, transaction_path(transaction.stub) %></td>
    <td><%= transaction.quantity %></td>
    <td><%= link_to transaction.ts, trade_path(transaction.ts) %></td>
    <%= td_money(transaction.price,4) %>
    <%= td_money(transaction.gross_principal,2) %>
    <%= td_money(transaction.commission,2) %>
    <%= td_money(transaction.sec_fee,2) %>
    <%= td_money(transaction.fees,2) %>
    <%= td_money(transaction.net_principal,2) %>
    <td><%= transaction.jottings %></td>
  </tr>
<% end %>

</tbody>
</table>
